@model IEnumerable<Monarch.Models.ButterflyTrackingContext.SightingFileError>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SightingFileUpload.SightingFileUploadId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Error)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SightingFileUpload.SightingFileUploadId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Error)
        </td>
    </tr>
}

</table>
